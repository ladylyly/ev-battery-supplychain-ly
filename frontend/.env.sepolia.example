# Frontend Environment Variables for Sepolia Testnet
# Copy this file to .env.sepolia and update with your deployed contract addresses

# --- Network ---
REACT_APP_RPC_URL=https://ethereum-sepolia.publicnode.com
REACT_APP_CHAIN_ALIAS=11155111
REACT_APP_CHAIN_ID=11155111
REACT_APP_RAILGUN_CHAIN=ethereum
REACT_APP_RAILGUN_NETWORK=Ethereum_Sepolia
REACT_APP_RAILGUN_SCAN_RPC_URL=https://ethereum-sepolia.publicnode.com

# --- Contracts (update after deployment) ---
REACT_APP_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000
REACT_APP_WETH_ADDRESS=0xfff9976782d46CC05630d1f6eBAb18b2324d6B14

# --- Pinata IPFS (get from https://www.pinata.cloud/) ---
REACT_APP_PINATA_JWT=your-pinata-jwt-token-here

# --- Backend API (optional) ---
REACT_APP_RAILGUN_API_URL=http://localhost:3001

# --- ZKP Backend ---
REACT_APP_ZKP_BACKEND_URL=http://localhost:5010

# --- Misc ---
SKIP_PREFLIGHT_CHECK=true
GENERATE_SOURCEMAP=false
REACT_APP_SUPPRESS_WARNINGS=true
REACT_APP_VERBOSE=false
REACT_APP_LOGS_CHUNK_BLOCKS=8

# Feature flags
REACT_APP_RAILGUN_WALLET_MODE=false
REACT_APP_SHIELD_STRATEGY=sdk
